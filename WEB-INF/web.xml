<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>
	Outils</display-name>
	<context-param>
		<param-name>HOST_SGBD</param-name>
		<param-value>noumea</param-value>
	</context-param>
	<context-param>
		<param-name>HOST_SGBD_ADMIN</param-name>
		<param-value>***REMOVED***</param-value>
	</context-param>
	<context-param>
		<param-name>HOST_SGBD_PWD</param-name>
		<param-value>***REMOVED***</param-value>
	</context-param>
	<servlet>
		<description>
		</description>
		<display-name>
		ExportFileServlet</display-name>
		<servlet-name>ExportFileServlet</servlet-name>
		<servlet-class>
		nc.mairie.exportfile.servlet.ExportFileServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		ImageDistillerServlet</display-name>
		<servlet-name>ImageDistillerServlet</servlet-name>
		<servlet-class>
		nc.mairie.outils.distiller.ImageDistillerServlet</servlet-class>
		<load-on-startup>-1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>ImageDistillerServlet</servlet-name>
		<url-pattern>/ImageDistillerServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ExportFileServlet</servlet-name>
		<url-pattern>/ExportFileServlet</url-pattern>
	</servlet-mapping>
</web-app>

